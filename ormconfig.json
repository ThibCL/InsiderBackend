{
   "type": "postgres",
   "name": "default",
   "url": "postgres",
   "synchronize": true,
   "logging": false,
   "entities": [
      "src/entity/**/*.js"
   ],
   "migrations": [
      "src/migration/**/*.js"
   ],
   "subscribers": [
      "src/subscriber/**/*.js"
   ],
   "cli": {
      "entitiesDir": "build/src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}
